{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    },
    {"src":"/api/scraper",
  "dest":"/api/scraper.js"}
  ],
  "crons": [
    {
      "path": "/api/scraper",
      "schedule": "0 0 * * *"
    }
  ]
}
