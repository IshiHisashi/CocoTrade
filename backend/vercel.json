{
  "version": 2,
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/scraper", "dest": "/api/scraper.js" },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "crons": [
    {
      "path": "/api/scraper",
      "schedule": "0 0 * * *"
    }
  ]
}
